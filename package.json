{
  "name": "advisor-frontend",
  "version": "2.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/RedHatInsights/insights-advisor-frontend.git"
  },
  "license": "Apache-2.0",
  "author": "Red Hat",
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/dev.webpack.config.js --env.prod=true --env.analyze=true --mode production",
    "build": "webpack --config config/dev.webpack.config.js --env analyze=false",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run travis:verify",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "test": "jest --verbose --passWithNoTests",
    "translations": "npm-run-all translations:*",
    "translations:extract": "npx formatjs extract ./src/*.js --out-file ./build/messages/src/Messages.json --extract-from-format-message-call",
    "translations:compile": "npx formatjs compile ./build/messages/src/Messages.json --out-file ./locales/translations.json",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.12.1",
    "@patternfly/react-core": "4.75.2",
    "@patternfly/react-icons": "4.7.16",
    "@patternfly/react-table": "4.19.5",
    "@patternfly/react-tokens": "4.9.16",
    "@react-pdf/renderer": "1.6.11",
    "@redhat-cloud-services/frontend-components": "2.4.18",
    "@redhat-cloud-services/frontend-components-charts": "2.4.2",
    "@redhat-cloud-services/frontend-components-inventory-insights": "2.6.5",
    "@redhat-cloud-services/frontend-components-notifications": "2.2.3",
    "@redhat-cloud-services/frontend-components-pdf-generator": "2.3.7",
    "@redhat-cloud-services/frontend-components-remediations": "2.3.0",
    "@redhat-cloud-services/frontend-components-translations": "2.2.1",
    "@redhat-cloud-services/frontend-components-utilities": "2.2.7",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.1",
    "classnames": "2.2.6",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "17.0.1",
    "react-content-loader": "5.1.3",
    "react-dom": "17.0.1",
    "react-intl": "5.8.8",
    "react-markdown": "5.0.2",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-async-to-generator": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-flow": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@formatjs/cli": "2.13.5",
    "axios": "0.21.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.1",
    "babel-loader": "8.1.0",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-lodash": "3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "6.3.0",
    "css-loader": "3.6.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^5.0.0-alpha.13",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.1",
    "lodash": "4.17.20",
    "mini-css-extract-plugin": "^1.3.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.4",
    "seamless-immutable": "7.1.4",
    "style-loader": "^2.0.0",
    "stylelint": "13.7.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "typescript": "4.0.5",
    "webpack": "5.4.0",
    "webpack-bundle-analyzer": "4.1.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^5.3.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "insights": {
    "appname": "advisor"
  }
}
